@using Microsoft.AspNet.Identity
@model RajaMotors.Web.ViewModels.ClientViewModel
@{
    ViewBag.Title = "EditProfileView";
    Layout = "~/Views/Shared/_HomeLayoutRajaMotors.cshtml";
}   


<div class="well span12">
    <div class="span4" style="padding-left:30px;"> 
        <h4>Clients</h4>
    </div>     

    <div class="span11">
        <table class="table">
            <tr>
                <td>
                    @Html.LabelFor(model => model.ClientName, new { @class = "span3" }) 
                </td>
                <td>
                    @Html.Encode(Model.ClientName) 
                </td>
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.ClientMobile, new { @class = "span3" })
                </td>
                <td>
                    @Html.Encode(Model.ClientMobile) 
                </td> 
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.ClientAddress, new { @class = "span3" })
                </td>
                <td>
                    @Html.Encode(Model.ClientAddress) 
                </td>  
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.ClientAddress1, new { @class = "span3" })
                </td>
                <td>
                    @Html.Encode(Model.ClientAddress1) 
                </td>                 
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.ClientAddress2, new { @class = "span3" })
                </td>
                <td>
                    @Html.Encode(Model.ClientAddress2) 
                </td>  
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.ClientDateAdded, new { @class = "span3" })
                </td>
                <td>
                    @Html.Encode(Model.ClientDateAdded) 
                </td>  
            </tr>
            <tr>
                <td>  
                    <a href="/client/ClientList" >back</a>
                </td>
            </tr>             
        </table>
    </div>
</div>



