@using Microsoft.AspNet.Identity
@model IEnumerable<RajaMotors.Web.ViewModels.VehicleViewModel>
@{
    ViewBag.Title = "Cars";
    Layout = "~/Views/Shared/_HomeLayoutRajaMotors.cshtml";
}
<h2>Cars</h2>
<div class="container-fluid">
    <div class="row-fluid">
        <div class="col_w620_content">
            <fieldset>
                <legend>Car List</legend>
                <table class="table">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Mobile</th>
                            <th>Model Name</th>
                            <th>Model Number</th>
                            <th>DateAdded</th>
                            <th>DateModified</th>                             
                            <th>Active</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in Model)
            {
                            <tr>
                                <td>
                                    @item.Client.ClientName
                                </td>
                                <td>
                                    @item.Client.ClientMobile
                                </td>
                                <td>
                                    @item.VehicleModelName
                                </td>
                                <td>
                                    @item.VehicleModelNumber
                                </td>
                                <td>
                                    @item.VehicletDateAdded
                                </td>
                                <td>
                                    @item.VehicletDateModified
                                </td>
                                <td>
                                    @item.VehicleIsActive
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>

            </fieldset>
        </div>
    </div>
</div>