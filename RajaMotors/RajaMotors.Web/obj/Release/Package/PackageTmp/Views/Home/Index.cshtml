@using System.Web.Mvc.Html
@model RajaMotors.Web.ViewModels.ServicePageViewModel 

@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_HomeLayoutRajaMotors.cshtml";
}
<div class="container-fluid">
    <div class="row-fluid">
        <div class="col_w620_content">
            <h4>Cars pending first Service</h4>
            @Html.Partial("_VehicleList", Model.ListVehiclesServiceDue)
        </div>

        <div class="col_w620_content">
            <h4>Cars with next Service due</h4>
            @Html.Partial("_ServiceList", Model.serviceList)
        </div>
    </div>
</div>

